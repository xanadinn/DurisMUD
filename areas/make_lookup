echo
echo "Making lookup table."
echo "--------------------"
echo "Making mobiles lookup table."
gawk -f lookup.awk target='~' world.mob > ../lib/misc/lookup.mob
echo "Making objects lookup table."
gawk -f lookup.awk target='~' world.obj > ../lib/misc/lookup.obj
echo "Making world lookup table."
gawk -f lookup.rawk target='~' world.wld > ../lib/misc/lookup.wld
echo "Making zones lookup table."
gawk -f lookup.rawk target='~' world.zon > ../lib/misc/lookup.zon
echo "Lookup table done."
gawk -f lookup_with_limits.awk target='~' world.zon > ../lib/misc/lookup_with_limits.zon
echo "Lookup zone table with limits done."
echo "Converting lookup mob to unix format..."
dos2unix ../lib/misc/lookup.mob
echo "....done!"

echo
